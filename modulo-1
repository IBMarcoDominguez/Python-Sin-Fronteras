# import modulos as xs

from modulos import saludo, mascotas
from camelcase import CamelCase
# print(xs.mascotas)
# xs.saludo("Marco")

saludo("Marco") 
print(mascotas)

c = CamelCase()
s = "Esta oraci√≥n necesita Camelcase"
camelcase = c.hump(s)
print(camelcase)
